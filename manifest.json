{
  "manifest_version": 2,

  "name": "youtube2HTML5",
  "description": "Reload the youtube page in HTML5",
  "version": "1.0",
  "manifest_version": 2,

  // "browser_action": {
  //   "default_icon": "icon.png"
  // },
  "icons": { 
            "16":"icon.png", 
            "48":"icon.png", 
            "128":"icon.png" 
        }, 

  // "permissions": [
  //   "tabs"],

   "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*"],
     
      "js": ["jquery.js", "script.js"]
    }],

   "web_accessible_resources": ["script.js","htmlify.png"]
}
